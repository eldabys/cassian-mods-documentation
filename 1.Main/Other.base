views:
  - type: table
    name: Table
    filters:
      and:
        - file.ext == "md"
        - '!file.tags.containsAny("Status/Maintenance", "Status/Active")'
        - '!file.tags.contains("Disambiguation")'
    sort:
      - property: file.name
        direction: DESC
  - type: table
    name: View
